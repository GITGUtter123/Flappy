{
  "manifest_version": 3,
  "name": "Super Flappy Bird",
  "version": "2.0",
  "description": "A polished Flappy Bird game with smooth animations and multiple themes",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "data:image/svg+xml;base64,",
      "32": "data:image/svg+xml;base64,",
      "48": "data:image/svg+xml;base64,",
      "128": "data:image/svg+xml;base64,"
    }
  },
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "32": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  },
  "permissions": ["storage"]
}